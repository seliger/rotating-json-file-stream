{
  "name": "rotating-json-file-stream",
  "version": "1.0.0",
  "description": "Opens a stream accepting JSON objects. Streams JSON object array rotated by interval and/or size.",
  "scripts": {
    "all": "npm run npmignore && npm run eslint && npm run coverage",
    "coverage": "TZ=\"America/Indiana/Indianapolis\" ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --recursive test",
    "eslint": "./node_modules/.bin/eslint *.js test/*js",
    "npmignore": "echo '.codeclimate.yml\\n.eslintrc\\n.gitignore\\n.travis.yml\\ntest' > .npmignore ; cat .gitignore >> .npmignore",
    "test": "TZ=\"America/Indiana/Indianapolis\" ./node_modules/.bin/_mocha test"
  },
  "bugs": "https://github.com/seliger/rotating-json-file-stream/issues",
  "repository": "https://github.com/seliger/rotating-json-file-stream",
  "keywords": [
    "json",
    "file",
    "stream",
    "rotate"
  ],
  "engines": {
    "node": ">=0.11"
  },
  "author": "Corey S. Seliger <corey@seligerfamily.org> (https://github.com/seliger)",
  "contributors": [
    "iccicci",
    "allevo"
  ],
  "license": "MIT",
  "readmeFilename": "README.md",
  "devDependencies": {
    "eslint": "3.19.0",
    "istanbul": "0.4.5",
    "mocha-istanbul": "0.3.0",
    "mocha": "3.4.2"
  }
}
